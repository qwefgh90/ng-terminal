{
  "name": "ng-terminal-app",
  "version": "0.0.0",
  "scripts": {
    "start": "ng build && ng serve",
    "build": "npm run copy-readme && ng build",
    "lib-build": "npm run copy-readme && ng build ng-terminal",
    "travis-test": "ng test ng-terminal --watch false --browsers ChromeHeadless",
    "lib-test": "ng test ng-terminal",
    "gh-pages": "npm run lib-build && ng build --prod --output-path docs --base-href /ng-terminal/",
    "copy-readme": "copyfiles README.md projects/ng-terminal/",
    "patch-up": "cd projects/ng-terminal/ && npm version patch",
    "minor-up": "cd projects/ng-terminal/ && npm version minor",
    "major-up": "cd projects/ng-terminal/ && npm version major",
    "publish": "cd dist/ng-terminal && npm publish",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "add-licenses": "license-checker > LICENSE-THIRD-PARTY"
  },
  "dependencies": {
    "@angular/animations": "^7.2.12",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "angular-resizable-element": "^3.2.6",
    "copyfiles": "^2.1.0",
    "core-js": "^2.5.4",
    "ngx-json-viewer": "^2.4.0",
    "rxjs": "6.5.5",
    "tslib": "^1.13.0",
    "xterm": "^4.2.0",
    "xterm-addon-fit": "^0.4.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.3.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.36.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "license-checker": "^25.0.1"
  }
}
