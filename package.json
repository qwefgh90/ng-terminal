{
  "name": "ng-terminal-app",
  "version": "0.0.0",
  "scripts": {
    "start": "ng build && ng serve",
    "build": "npm run copy-readme && ng build",
    "lib-build": "npm run copy-readme && ng build ng-terminal",
    "travis-test": "ng test ng-terminal --watch false --browsers ChromeHeadless",
    "lib-test": "ng test ng-terminal",
    "gh-pages": "npm run lib-build && ng build --prod --output-path docs --base-href /ng-terminal/",
    "copy-readme": "copyfiles README.md projects/ng-terminal/",
    "patch-up": "cd projects/ng-terminal/ && npm version patch",
    "minor-up": "cd projects/ng-terminal/ && npm version minor",
    "major-up": "cd projects/ng-terminal/ && npm version major",
    "publish": "cd dist/ng-terminal && npm publish",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "add-licenses": "license-checker > LICENSE-THIRD-PARTY"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "angular-resizable-element": "^3.3.2",
    "copyfiles": "^2.1.0",
    "core-js": "^2.5.4",
    "ngx-json-viewer": "^2.4.0",
    "rxjs": "6.5.5",
    "tslib": "^1.13.0",
    "xterm": "^4.6.0",
    "xterm-addon-fit": "^0.4.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "license-checker": "^25.0.1",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.1",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
